import UIKit

class ViewController: UIViewController {

    override func viewDidLoad() {
        super.viewDidLoad()
        // Do any additional setup after loading the view, typically from a nib.
    }
    
    // IB OUTLETS

    @IBAction func IBStepperAction(_ sender: UIStepper) {
        stepperDisplayLabel.text=String(sender.value);
    }
    @IBOutlet weak var IBStepperOUTLET: UIStepper!
    
    @IBAction func IBSliderACTION(_ sender: UISlider) {
    }
    @IBOutlet weak var IBSliderOUTLET: UISlider!
    
    @IBOutlet weak var stepperDisplayLabel: UILabel!
    @IBOutlet weak var sliderDisplayLabel: UILabel!
    
}

